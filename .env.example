# Copy to .env before running locally
# cp .env.example .env

# Ethereum network RPC URL (local or remote)
RPC_URL=http://localhost:8545

# Network name (e.g., Ethereum mainnet, Rinkeby, or custom networks)
NETWORK=development

# Optional API key for external services
API_KEY=

# Log level for debugging (info | debug | warn)
LOG_LEVEL=info
